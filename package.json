{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "A Discord bot built with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc && node post-build.js",
    "deploy": "node deploy-commands.js",
    "test": "node test-dashboard-complete.js",
    "test:interactive": "node test-dashboard-complete.js --interactive",
    "test:commands": "node test-bot-commands.js",
    "dashboard": "cd client && npm start",
    "dashboard:dev": "cd client && PORT=3002 npm start",
    "dashboard:build": "cd client && npm run build",
    "dashboard:demo": "node start-dashboard-demo.js",
    "build:prod": "set NODE_ENV=production && npm run build",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "build:docker": "docker build -t discord-bot .",
    "prod:full": "npm run build:prod && npm run dashboard:build",
    "railway:build": "npm run build && cd client && npm install && npm run build",
    "railway:start": "node dist/index.js",
    "heroku-postbuild": "npm run build && cd client && npm install && npm run build",
    "watch": "tsc -w",
    "deploy-commands": "ts-node src/deploy-commands.ts",
    "clear-commands": "ts-node src/clear-commands.ts",
    "reset-commands": "ts-node src/reset-all-commands.ts",
    "clear-all-commands": "ts-node src/clear-all-commands.ts",
    "fix-commands": "ts-node src/fix-commands.ts",
    "fix-duplicates": "ts-node src/fix-duplicates.ts",
    "direct-fix": "node src/direct-fix.js",
    "ts-fix": "ts-node src/direct-fix.ts",
    "db:init": "tsx scripts/init-database.ts",
    "setup-dashboard": "node setup-dashboard.js",
    "server": "node server.js",
    "deploy:production": "npm run prod:full && bash scripts/deploy-production.sh",
    "mcp:start": "node scripts/start-mcp-server.js",
    "mcp:build": "tsc && node post-build.js",
    "mcp:dev": "ts-node src/mcp/discord-bot-mcp-server.ts",
    "mcp:test": "node scripts/test-mcp-server.js",
    "mcp:enhanced": "node scripts/start-enhanced-mcp-server.js",
    "mcp:enhanced:dev": "ts-node src/mcp/enhanced-discord-bot-mcp-server.ts",
    "mcp:enhanced:test": "node scripts/test-enhanced-mcp-server.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "postinstall": "npm run build"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^1.11.2",
    "@discordjs/rest": "^2.5.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "@modelcontextprotocol/server-filesystem": "^2025.7.1",
    "@modelcontextprotocol/server-memory": "^2025.4.25",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@mui/x-data-grid": "^8.3.1",
    "@types/node-schedule": "^2.1.7",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "better-sqlite3": "^11.10.0",
    "canvas": "^3.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "discord-api-types": "^0.38.3",
    "discord.js": "^14.14.1",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mcp-starter": "^0.1.0",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "node-schedule": "^2.1.1",
    "openai": "^4.102.0",
    "path-to-regexp": "^6.2.0",
    "reflect-metadata": "^0.2.1",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "tsx": "^4.7.1",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.19",
    "@types/node-fetch": "^2.6.12",
    "@types/ws": "^8.5.10",
    "nodemon": "^3.1.10",
    "playwright": "^1.54.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
