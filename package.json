{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "A Discord bot built with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc -w",
    "deploy-commands": "ts-node src/deploy-commands.ts",
    "clear-commands": "ts-node src/clear-commands.ts",
    "reset-commands": "ts-node src/reset-all-commands.ts",
    "dashboard": "cd simple-dashboard && npm start",
    "dashboard:dev": "cd simple-dashboard && PORT=3003 npm start",
    "setup-dashboard": "node setup-dashboard.js",
    "server": "node server.js"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^1.11.2",
    "@discordjs/rest": "^2.5.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@mui/x-data-grid": "^8.3.1",
    "@types/jsonwebtoken": "^9.0.9",
    "axios": "^1.9.0",
    "better-sqlite3": "^11.9.1",
    "canvas": "^3.1.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^4.1.0",
    "discord-api-types": "^0.38.3",
    "discord.js": "^14.14.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.14.1",
    "node-fetch": "^2.7.0",
    "node-schedule": "^2.1.1",
    "openai": "^4.98.0",
    "react-router-dom": "^7.6.0",
    "session-file-store": "^1.5.0",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.17.10",
    "@types/helmet": "^0.0.48",
    "@types/node": "^20.17.50",
    "@types/node-fetch": "^2.6.11",
    "@types/node-schedule": "^2.1.7",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "express-session": "^1.18.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
