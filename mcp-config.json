{
  "mcpServers": {
    "discord-bot": {
      "command": "node",
      "args": ["dist/mcp/discord-bot-mcp-server.js"],
      "env": {
        "DISCORD_TOKEN": "${DISCORD_TOKEN}",
        "NODE_ENV": "production"
      }
    },
    "enhanced-discord-bot": {
      "command": "node",
      "args": ["dist/mcp/enhanced-discord-bot-mcp-server.js"],
      "env": {
        "DISCORD_TOKEN": "${DISCORD_TOKEN}",
        "API_BASE_URL": "http://localhost:3001",
        "NODE_ENV": "production"
      }
    },
    "playwright": {
      "url": "http://localhost:3006/mcp"
    },
    "filesystem": {
      "command": "npx",
      "args": ["@modelcontextprotocol/server-filesystem", "--root", "."]
    },
    "memory": {
      "command": "npx",
      "args": ["@modelcontextprotocol/server-memory"]
    }
  }
}